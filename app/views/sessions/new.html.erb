<h1>Login</h1>
<%= form_for(:session, url: login_path) do |f| %>
      <div class="field">
        <%= f.label :user_id %>
        <%= f.text_field :user_id, id: :user_user_id %>
      </div>
      <div class="field">
        <%= f.label :password %>
        <%= f.password_field :password %>
      </div>
      <div class="actions">
        <%= f.submit "Log in" %>
      </div>
    <% end %>
    <p>New user? <%= link_to "Sign up now!", new_user_path %></p>
<p>Find me in app/views/sessions/new.html.erb</p>
